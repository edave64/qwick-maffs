{
	"compilerOptions": {
		"target": "ES5",
		"module": "Preserve",
		"declaration": true,
		"outDir": "./dist",
		"isolatedModules": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"alwaysStrict": true,
		"skipLibCheck": true,
		"emitDeclarationOnly": true
	},
	"include": ["./src/**/*"]
}
